<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/authorizationFragment">

    <fragment
        android:id="@+id/authorizationFragment"
        android:name="com.app.Account.Autorization.AuthorizationFragment"
        android:label="fragment_authorization"
        tools:layout="@layout/fragment_authorization" >
        <action
            android:id="@+id/action_authorizationFragment_to_scheduleFragment"
            app:destination="@id/scheduleFragment" />
    </fragment>
    <fragment
        android:id="@+id/accountFragment"
        android:name="com.app.Account.AccountFragment"
        android:label="fragment_account"
        tools:layout="@layout/fragment_account" >
        <action
            android:id="@+id/action_accountFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
        <action
            android:id="@+id/action_accountFragment_to_scheduleFragment"
            app:destination="@id/scheduleFragment" />
    </fragment>
    <fragment
        android:id="@+id/favoritesFragment"
        android:name="com.app.Favorites.FavoritesFragment"
        android:label="fragment_favorites"
        tools:layout="@layout/fragment_favorites" />
    <fragment
        android:id="@+id/flashCardFragment"
        android:name="com.app.FlashCard.FlashCardFragment"
        android:label="fragment_flash_card"
        tools:layout="@layout/fragment_flash_card" >
        <action
            android:id="@+id/action_flashCardFragment_to_flashCardsFragment"
            app:destination="@id/flashCardsFragment" />
        <action
            android:id="@+id/action_flashCardFragment_to_addFlashCardFragment"
            app:destination="@id/addFlashCardFragment" />
        <action
            android:id="@+id/action_flashCardFragment_to_subjectFragment"
            app:destination="@id/subjectFragment" />
    </fragment>
    <fragment
        android:id="@+id/scheduleFragment"
        android:name="com.app.Schedule.ScheduleFragment"
        android:label="fragment_schedule"
        tools:layout="@layout/fragment_schedule" >
        <action
            android:id="@+id/action_scheduleFragment_to_subjectsFragment"
            app:destination="@id/subjectsFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.app.Account.Settings.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" >
        <action
            android:id="@+id/action_settingsFragment_to_changeSettingsFragment"
            app:destination="@id/changeSettingsFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_supportFragment"
            app:destination="@id/supportFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_aboutFragment"
            app:destination="@id/aboutFragment" />
    </fragment>
    <fragment
        android:id="@+id/subjectFragment"
        android:name="com.app.Subjects.SubjectFragment"
        android:label="fragment_subject"
        tools:layout="@layout/fragment_subject" >
        <action
            android:id="@+id/action_subjectFragment_to_textbookFragment"
            app:destination="@id/textbookFragment" />
        <action
            android:id="@+id/action_subjectFragment_to_flashCardFragment"
            app:destination="@id/flashCardFragment" />
        <action
            android:id="@+id/action_subjectFragment_to_subjectsFragment"
            app:destination="@id/subjectsFragment" />
    </fragment>
    <fragment
        android:id="@+id/subjectsFragment"
        android:name="com.app.Subjects.SubjectsFragment"
        android:label="fragment_subjects"
        tools:layout="@layout/fragment_subjects" >
        <action
            android:id="@+id/action_subjectsFragment_to_subjectFragment"
            app:destination="@id/subjectFragment" />
    </fragment>
    <fragment
        android:id="@+id/textbookFragment"
        android:name="com.app.Subjects.Textbook.TextbookFragment"
        android:label="fragment_textbook"
        tools:layout="@layout/fragment_textbook" >
        <action
            android:id="@+id/action_textbookFragment_to_subjectFragment"
            app:destination="@id/subjectFragment" />
    </fragment>
    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.app.Account.Settings.About.AboutFragment"
        android:label="fragment_about"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/changeSettingsFragment"
        android:name="com.app.Account.Settings.ChangeSettingsFragment"
        android:label="fragment_change_settings"
        tools:layout="@layout/fragment_change_settings" />
    <fragment
        android:id="@+id/supportFragment"
        android:name="com.app.Account.Settings.SupportFragment"
        android:label="fragment_support"
        tools:layout="@layout/fragment_support" >
        <action
            android:id="@+id/action_supportFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/flashCardsFragment"
        android:name="com.app.FlashCard.FlashCardsFragment"
        android:label="fragment_flash_cards"
        tools:layout="@layout/fragment_flash_cards" >
        <action
            android:id="@+id/action_flashCardsFragment_to_flashCardFragment"
            app:destination="@id/flashCardFragment" />
    </fragment>
    <fragment
        android:id="@+id/addFlashCardFragment"
        android:name="com.app.FlashCard.AddFlashCardFragment"
        android:label="fragment_add_flash_card"
        tools:layout="@layout/fragment_add_flash_card" >
        <action
            android:id="@+id/action_addFlashCardFragment_to_flashCardFragment"
            app:destination="@id/flashCardFragment" />
    </fragment>
</navigation>